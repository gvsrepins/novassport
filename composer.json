{
    "name": "kristories/novassport",
    "description": "A Laravel Nova tool to manage API Authentication (Passport).",
    "keywords": [
        "laravel",
        "nova",
        "passport",
        "oauth",
        "oauth2",
        "authentication",
        "auth"
    ],
    "license": "MIT",
    "require": {
       "php": "^7.2.5",
        "laravel/nova": "^3.0",
        "laravel/framework": "^7.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kristories\\Novassport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kristories\\Novassport\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
